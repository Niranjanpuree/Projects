@{
    ViewData["Title"] = "Far Clause";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .farclause_grid .btn-link {
        color: #00649B;
    }

        .farclause_grid .btn-link:hover {
            text-decoration: underline;
        }
</style>
@section breadcrumb
    {
    <li class="breadcrumb-item"><a href="/Admin/Settings">Settings</a></li>
    <li class="breadcrumb-item"><a href="#">Far Clause</a></li>
}

<div id="farClauseGrid"></div>
<div id="dialog">
    <div class="content"></div>
</div>

@section Scripts {

    <script type="text/javascript">
        (function ()
        {
            window.farClause.farClauseList('Far Clause List', '@User.Claims.Where(c=>c.Type.ToString() == "fullName").ToList()[0].Value');
        })()
    </script>
}