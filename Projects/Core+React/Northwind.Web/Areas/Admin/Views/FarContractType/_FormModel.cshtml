        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="FarContractTypeGuid" class="form-control" />
        <div class="row">
            <div class="col-md-6 form-group">
                <label asp-for="Code" class=" fontbold requiredAstreicks control-label"></label>
                <input asp-for="Code" class="form-control" />
