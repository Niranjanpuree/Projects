@model Northwind.Web.Models.ViewModels.Contract.ContractQuestionaireViewModel
<div class="">
    <div class="form-group">
        <div class="control-label control-label-read">
            Does the contract contain FAR clause 52.216-7 and or FAR clause 52.232-7
        </div>
        <h6>
            @Model.IsFARclauseStatus
        </h6>
    </div>

    <div class="row align-items-end">
        <div class="col-md-6">
            <div class="form-group">
                <div class="control-label control-label-read">
                    GSA Schedule Sales & Industrial Funding Fee (GSAR 552.238-74) :
                </div>
                <h6>
                    @Model.IsGSAschedulesaleStatus
                </h6>
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <div class="control-label control-label-read">
                    Date of Last Report
                </div>
                <h6>
                    @(Model.GSA_LastReportDate == null ? "Not Provided" :
                            Model.GSA_LastReportDate.Value.ToShortDateString())
                </h6>
            </div>
        </div>
        <div class="col-md-3">
            <div class="form-group">
                <div class="control-label control-label-read">
                    Date of Next Report
                </div>
                <h6>
                    @(Model.GSA_NextReportDate == null ? "Not Provided" :
                            Model.GSA_NextReportDate.Value.ToShortDateString())
                </h6>
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="control-label control-label-read">
            Government Furnished Property (52.219-9)
        </div>
        <h6>
            @Model.IsGovtFurnishedPropertyStatus
        </h6>
    </div>

    <div class="form-group">
        <div class="control-label control-label-read">
            Service Contract Reporting
        </div>
