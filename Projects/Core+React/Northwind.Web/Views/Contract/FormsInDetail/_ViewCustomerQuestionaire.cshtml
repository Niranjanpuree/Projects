    <div class="row align-items-end">
        <div class="col-md-6 ">
            <div class="form-group">
                <div class="control-label control-label-read">
                    CPARS (FAR Subpart 42.215)
                </div>
                <h6>
                    @Model.IsCPARSStatus
                </h6>
            </div>
        </div>

        <div class="col-md-3 ">
            <div class="form-group">
                <div class="control-label control-label-read">
                    Date of Last Report
                </div>
                <h6>
                    @(Model.CPARS_LastReportDate == null ? "Not Provided" :
                            Model.CPARS_LastReportDate.Value.ToShortDateString())
                </h6>
            </div>
        </div>
        <div class="col-md-3 ">
            <div class="form-group">
                <div class="control-label control-label-read">
