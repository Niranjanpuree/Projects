                </div>
                <h6>
                    @(Model.GQAC_NextReportDate == null ? "Not Provided" :
                            Model.GQAC_NextReportDate.Value.ToShortDateString())
                </h6>
            </div>
        </div>
    </div>
    <div class="row align-items-end">
        <div class="col-md-3">
            <div class="form-group">
                <div class="control-label control-label-read">
