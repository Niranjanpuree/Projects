@model List<Northwind.Core.Entities.ContractRefactor.ContractResourceFile>
<table class="table">
    <thead>
        <tr>
            <th scope="col">File Name</th>
            <th scope="col">Description</th>
            <th scope="col">Size</th>
            <th scope="col">Updated On</th>
            <th scope="col">Updated By</th>

        </tr>
    </thead>
    <tbody>

        @foreach (var data in Model)
        {
        <tr>
            <td><a href="/ContractNotice/DownloadFile?id=@data.FilePath">@data.UploadFileName</a></td>
            <td>@data.Description</td>
            <td>@data.FileSize</td>
            <td>@data.UpdatedOn.ToString("dd/MM/yyyy")</td>
            <td> @data.UpdatedByName</td>
        </tr>
        }
       
    </tbody>
</table>
   





